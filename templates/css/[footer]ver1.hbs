#footer {
		height: 120px;
		width: 100%;
		background: #000;
    	z-index: 9;
	    margin-top: -25px;
    	.container {
    		// position: relative;
    		// &:before {
    		// 	content: '';
    		// 	position: absolute;
    		// 	width: 316px;
    		// 	height: 219px;
    		// 	background: $bonsai center center;
    		// 	left: -145px;
			   //  top: -170px;
    		// }
    	}
		.logo {
		    display: block;
		    height: 39px;
		    float: left;
		    margin-top: 35px;
		    &.gosu {
	    	    width: 162px;
			    height: 42px;
			    background: url(../images/logo_gosu.png) no-repeat center center;
			    background-size: contain;
			    margin-left: 2px;
		    }
		    &.cubinet {
	    	    width: 238px;
			    background: url(../images/logo_cubinet.png) no-repeat center center;
			    background-size: contain;
		    }
		}
		.address {
		    width: 575px;
		    height: 81px;
		    float: left;
		    padding-top: 30px;
		    a {
	    	    color: #fff;
			    font-size: 14px;
			    font-family: Tahoma;
			    padding: 0 2px 0 5px;
			    line-height: 1;
		    }
		    p {
	    	    text-align: center;
			    width: 100%;
			    font-size: 14px;
			    font-family: Tahoma;
			    float: left;
			    color: #fff;
			    margin: 6px 0px 0px 0px;
		    }
		    .menu_footer {
		    	text-align: center;
			    margin-left: 15px;
			    li {
		    	    float: left;
				    margin: 1px 4px;
				    border-left: 1px solid #fff;
				    &:nth-child(1) {
				    	border-left: none;
				    }
				    a {
				    	width: 100%;
					    height: 100%;
					    display: block;
					    &:hover {
					    	color: #ff9427;
					    }
				    }
			    }
		    }
		}
	}